%fieldset
  %ol
    %li.string.optional
      %label=label
      = text_field model,:"#{field}_id".to_sym, :size =>6
      = javascript_include_tag('jq-autocomplete/autocomplete')
      %input.autocomplete{:id=>"#{field}_name",:table => field,:AUTOCOMPLETE =>"OFF",
        :value=> if action_name == "edit";data.try(field).try(:name);end}

